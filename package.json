{
  "name": "@optimisegroup/valkyrie",
  "version": "0.4.0",
  "description": "The iconography of Sippy.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "icons": "npm run icons:clean && npm run icons:ts",
    "icons:clean": "svgo -f ./icons -o ./icons",
    "icons:ts": "svg-to-ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "npm run icons && npm run build && npm run scss",
    "scss": "npm run scss:compile && npm run scss:prefix && npm run scss:copy",
    "scss:compile": "node-sass src -o dist --output-style compressed",
    "scss:copy": "bundle-scss --mask \"src/valkyrie.scss\" --dest \"dist/valkyrie.scss\"",
    "scss:prefix": "postcss -u autoprefixer -r dist/*.css --map",
    "trf:generate": "fantasticon",
    "trf:icons:clean": "oslllo-svg-fixer -s icons -d icons",
    "trf:icons:js": "svgtojs ./icons ./dist/js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/optimise-group/valkyrie.git"
  },
  "files": [
    "/icons",
    "/dist"
  ],
  "keywords": [
    "icons",
    "svg-icons",
    "iconography"
  ],
  "author": "",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/optimise-group/valkyrie/issues"
  },
  "homepage": "https://github.com/optimise-group/valkyrie#readme",
  "peerDependencies": {
    "react": ">=16.x",
    "clsx": ">=1.x"
  },
  "devDependencies": {
    "@types/react": "^17.0.15",
    "autoprefixer": "^10.3.1",
    "bundle-scss": "^1.5.0",
    "clsx": "^1.1.1",
    "fantasticon": "^1.2.2",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "oslllo-svg-fixer": "^1.4.0",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.36.0",
    "svg-to-js-cli": "^1.3.1",
    "svg-to-ts": "^6.0.1",
    "svgo": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "browserslist": {
    "production": [
      "last 2 versions",
      "not dead",
      "not ie <= 11"
    ]
  }
}
